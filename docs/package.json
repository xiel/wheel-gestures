  "description": "examples and documentation for wheel-gestures package",
  "version": "0.2.0",
  "author": "Felix Leupold <felix@xiel.de>",
  "license": "MIT",
  "scripts": {
    "build": "tsc && gatsby build",
    "develop": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "prebuild": "yarn link-package",
    "predevelop": "yarn link-package",
    "postinstall": "patch-package",
    "link-package": "cd ..; yarn unlink; yarn link; cd docs; yarn link wheel-gestures;"
  },
