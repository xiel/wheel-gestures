{
  "name": "wheel-gestures",
  "main": "dist/wheel-gestures.cjs.js",
  "module": "dist/wheel-gestures.esm.js",
  "types": "dist/wheel-gestures.d.ts",
  "version": "0.0.0-development",
  "description": "wheel gestures and momentum detection",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "yarn dev",
    "test": "react-scripts test",
    "semantic-release": "semantic-release",
    "link-examples": "yarn unlink; yarn link; cd docs; yarn link wheel-gestures; cd .."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xiel/wheel-gestures.git"
  },
  "keywords": [
    "wheel",
    "scroll",
    "mouse",
    "trackpad",
    "gesture",
    "events"
  ],
  "author": "Felix Leupold <felix@xiel.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xiel/wheel-gestures/issues"
  },
  "homepage": "https://github.com/xiel/wheel-gestures#readme",
  "dependencies": {
    "throttle-debounce": "^2.1.0"
  },
  "files": [
    "dist/**/*",
    "!**/test/*"
  ],
  "devDependencies": {
    "@testing-library/react": "^10.0.1",
    "@types/jest": "^25.1.4",
    "@types/throttle-debounce": "^2.1.0",
    "bundlewatch": "^0.2.6",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-react-app": "*",
    "prettier": "^2.0.1",
    "react-scripts": "3.4.1",
    "rollup": "^2.1.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "semantic-release": "^17.0.4",
    "tslib": "^1.9.3",
    "typescript": "^3.8.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jx,tsx,ts}",
      "!**/node_modules/**",
      "!**/dist/**",
      "!**/build/**",
      "!**/*.config.{js,jx,tsx,ts}"
    ]
  },
  "publishConfig": {
    "access": "restricted"
  },
  "bundlewatch": {
    "files": [
      {
        "path": "dist/*.js",
        "maxSize": "5kB"
      }
    ]
  }
}
